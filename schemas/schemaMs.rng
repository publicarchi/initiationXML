                  <ref name="data.pointer"/>
               </zeroOrMore>
            </list>
         </attribute>
         <optional>
            <attribute name="type">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">décrit le type de renseignement donné sur
					le témoin.</a:documentation>
               <ref name="data.enumerated"/>
